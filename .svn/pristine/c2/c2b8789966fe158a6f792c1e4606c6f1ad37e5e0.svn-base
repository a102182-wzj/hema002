page {
    background-color: #F8F8F8;
}

.page {
    display: flex;
    flex-direction: column;
    padding-bottom: 45px;
    overflow-x: hidden;
}

/* .banner {
    width: 100%;
    background-color: #fff;
  } */

.banner {
    width: 100%;
    background-color: #ecf0fb;
    height: 360px;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

/* .banner image {
    width: 110%;
    height: 340px;
  } */

.banner image {
    width: 110%;
    height: 340px;
}

.images {
    background-color: #fff;
    border-bottom: 1px solid #e6e6e6;
    padding-bottom: 40px;
}

.images-title {
    margin-top: 40px;
    margin-left: 35px;
    font-size: 30px;
    color: #999798;
}

.images-items {
    display: flex;
    flex-direction: row;
    width: 100%;
    justify-content: center;
}

.images-item {
    width: 19%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    margin-top: 30px;
}

.images-item image {
    width: 90px;
    height: 90px;
}

.images-item view {
    margin-top: 10px;
    font-size: 30px;
}

.time {
    width: 100%;
    /* height: 100px; */
    line-height: 50px;
    /* padding-left: 35px; */
    background-color: #fff;
    color: #8d8b8c;
    font-size: 30px;
    display: flex;
    flex-direction: row;
    padding-top: 30px;
    padding-bottom: 30px;
}

.time-title {
    margin-left: 35px;
    width: 20%;
}

.time-content {
    width: 80%;
    line-height: 50px;
}

.content {
    background-color: #fff;
    padding-bottom: 100px;
    margin-top: 20px;
}

.content-items {
    padding-left: 35px;
    padding-right: 35px;
    margin-top: 30px;
    line-height: 180%;
    font-size: 30px;
}

.content-items2 {
    margin-left: 35px;
    margin-top: 40px;
    color: #f85c5d;
    font-size: 30px;
}

.content-items view {
    font-size: 30px;
}

.buttom {
    width: 100%;
    height: 90px;
    line-height: 90px;
    color: #fff;
    background-color: #f85c5d;
    text-align: center;
    border-radius: 0;
    position: fixed;
    bottom: 0;
}

.buttom::after {
    border: 0;
}

button::after {
    border-radius: 0;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.bar {
    background-color: #ecf0fb;
    color: #526381;
    height: 100px;
    width: 100%;
    text-align: center;
    line-height: 100px;
    font-size: 34px;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.barimage {
    width: 90px;
    height: 90px;
    margin-right: 20px;
}

.example {
    width: 100%;
    height: auto;
    min-height: 900px;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    background-color: #fff;
    padding-top: 30px;
    /* padding-bottom: 30px; */
}

.example-box {
    width: 90%;
    height: auto;
    /* min-height: 800px; */
    display: flex;
    flex-direction: column;
    margin-bottom: 30px;
    padding-bottom: 50px;
}

.dataItem {
    display: flex;
    flex-direction: row;
    margin: 10px 0;
    /* justify-content: flex-end; */
    /* justify-content: flex-start; */
}

.headimg image {
    width: 70px;
    height: 70px;
}

.helfblock-left {
    width: 20px;
    height: 20px;
    background-color: #fff;
    border-left: 5px solid #5a94e6;
    border-bottom: 5px solid #5a94e6;
    transform: rotate(45deg);
    position: relative;
    left: 14px;
    top: 30px;
}

.helfblock-right {
    width: 20px;
    height: 20px;
    background-color: #c5d8f6;
    /* border-right: 5px solid #96add5;
    border-top: 5px solid #96add5; */
    transform: rotate(45deg);
    position: relative;
    right: 14px;
    top: 30px;
}

.chating {
    display: flex;
    flex-direction: column;
}

.chating view {
    margin: 5px 0;
}

.defaultImg {
    width: 250px;
    height: 250px;
    margin: 5px 0;
    border: 5px solid #96add5;
    border-radius: 20px;
}

.down {
    width: 400px;
    height: 200px;
    border: 5px solid #96add5;
    border-radius: 20px;
}

.name {
    font-size: 25px;
    margin: 5px 20px;
    padding-left: 20px;
    padding-right: 20px;
}

.reply-box {
    display: flex;
    flex-direction: row;
}

.reply {
    width: auto;
    min-width: 20px;
    max-width: 400px;
    height: auto;
    min-height: 60px;
    background-color: #94eb68;
    margin: 10px 10px 30px 10px;
    border-radius: 20px;
    word-break: break-all;
    padding: 20px;
    font-size: 28px;
}

.ontrial {
    margin-top: 10px;
    margin-bottom: 30px;
    width: 90%;
    height: 80px;
    /* background-color: #a2bbe3; */
    background-color: #ee857f;
    color: #fff;
    font-size: 18px;
    text-align: center;
    line-height: 80px;
    border-radius: 40px;
    position: fixed;
    bottom: 0;
    z-index: 100;
}

.emoji {
    width: 35px;
    height: 35px;
    margin: 0 5px;
    margin-bottom: -5px;
}